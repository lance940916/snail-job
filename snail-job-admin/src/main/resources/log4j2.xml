<?xml version="1.0" encoding="UTF-8"?>
<Configuration name="baseConf" status="info" monitorInterval="30">

    <appenders>
        <Console name="Console">
            <PatternLayout>
                <Pattern>[%-5p] %d{yyyy-MM-dd HH:mm:ss,SSS} %t %X{JOB_LOG_ID} %c{1.}#%M[%L]: %m%n</Pattern>
            </PatternLayout>
        </Console>
    </appenders>

    <loggers>
        <root level="info">
            <AppenderRef ref="Console"/>
        </root>
    </loggers>

</Configuration>